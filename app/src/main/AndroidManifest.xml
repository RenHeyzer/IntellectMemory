<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.geektech.intellect_memory.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_memory"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_memory_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Intellect_memort">
        <activity
            android:name="com.geektech.intellect_memory.presentation.ui.activity.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Intellect_memort.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>